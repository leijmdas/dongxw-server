<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kunlong.dongxw.manager.dao.DictDatatypeMapper" >
	<resultMap id="BaseResultMap" type="com.kunlong.dongxw.manager.domain.DictDatatype" >
	    <id column="data_inner_id" property="dataInnerId" jdbcType="INTEGER" />	
